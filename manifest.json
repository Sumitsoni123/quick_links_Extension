{
    "name": "Traversy Launcher",
    "description": "Quick links for frequent Sites",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon2.png"
    },
    "icons": {
        "128": "images/icon.png"
    },
    "permissions": [
        "activeTab"
    ]
}